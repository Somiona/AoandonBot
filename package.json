{
  "name": "aoandon-bot",
  "version": "0.1.0",
  "private": true,
  "files": [
    ".env",
    "koishi.yml"
  ],
  "license": "AGPL-3.0",
  "workspaces": [
    "packages/*",
    "packages/@*/*",
    "plugins/*",
    "plugins/@*/*"
  ],
  "scripts": {
    "new": "koishi-scripts new",
    "setup": "koishi-scripts setup",
    "build": "yakumo build",
    "bump": "yakumo version",
    "dep": "yakumo upgrade",
    "pub": "yakumo publish",
    "dev": "cross-env NODE_ENV=development koishi start -r esbuild-register -r yml-register --watch",
    "start": "koishi start",
    "test": "jest",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prepare": "husky install",
    "pre-commit": "jest && lint-staged"
  },
  "lint-staged": {
    "plugins/**/src/*.ts": [
      "eslint --fix"
    ],
    "packages/*.ts": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@koishijs/client": "^5.0.3",
    "@koishijs/plugin-mock": "^2.3.2",
    "@koishijs/scripts": "^3.4.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.13",
    "esbuild-register": "npm:@shigma/esbuild-register@^1.1.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4",
    "yakumo": "^0.3.7",
    "yakumo-esbuild": "^0.3.19",
    "yakumo-esbuild-yaml": "^0.3.1",
    "yakumo-publish": "^0.3.1",
    "yakumo-publish-sync": "^0.3.2",
    "yakumo-tsc": "^0.3.7",
    "yakumo-upgrade": "^0.3.2",
    "yakumo-version": "^0.3.2",
    "yml-register": "^1.1.0"
  },
  "dependencies": {
    "@koishijs/cli": "^4.10.10",
    "@koishijs/plugin-adapter-discord": "^3.3.3",
    "@koishijs/plugin-adapter-feishu": "^1.0.0",
    "@koishijs/plugin-adapter-kook": "^3.4.4",
    "@koishijs/plugin-adapter-onebot": "^5.4.4",
    "@koishijs/plugin-adapter-telegram": "^3.5.4",
    "@koishijs/plugin-admin": "^1.1.5",
    "@koishijs/plugin-auth": "^2.5.0",
    "@koishijs/plugin-bind": "^1.3.2",
    "@koishijs/plugin-chat": "3.2.0",
    "@koishijs/plugin-commands": "^2.3.0",
    "@koishijs/plugin-console": "5.0.3",
    "@koishijs/plugin-database-sqlite": "^3.2.2",
    "@koishijs/plugin-dataview": "^2.2.0",
    "@koishijs/plugin-echo": "^2.0.5",
    "@koishijs/plugin-help": "^2.0.1",
    "@koishijs/plugin-insight": "^3.2.0",
    "@koishijs/plugin-locales": "^2.1.1",
    "@koishijs/plugin-logger": "^2.2.0",
    "@koishijs/plugin-market": "^1.7.5",
    "@koishijs/plugin-rate-limit": "^1.2.4",
    "@koishijs/plugin-sandbox": "^2.5.0",
    "@koishijs/plugin-status": "^7.0.0",
    "@myrtus/koishi-plugin-forward": "2.1.7",
    "@myrtus/koishi-plugin-tool": "2.1.3",
    "koishi": "^4.11.0",
    "koishi-plugin-gocqhttp": "^3.2.0",
    "koishi-plugin-migration": "^0.1.0",
    "koishi-plugin-nonebot": "1.0.2",
    "koishi-plugin-puppeteer": "^3.3.1",
    "koishi-thirdeye": "^11.1.20"
  }
}
